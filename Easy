/*** Easy Problem 1: Two Sum */

class Solution {
    public int[] twoSum(int[] nums, int target) {
        int [] res = new int[2];
        
        for (int i = 0; i < nums.length-1; i++){
            int a = target - nums[i];
            for (int j = 1; j < nums.length; j++){
                if (a == nums[j] && i!=j){
                    res[0] = i;
                    res[1] = j;
                    break;
                }
            }
        }
        return res;
    }
}
